FoamFile
{
    version     2.0;
    format      binary;
    class       surfaceScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "41.4";
    object      meshPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];
oriented        oriented;


internalField   uniform 0;

boundaryField
{
    AMI0
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    AMI1
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    Inlet
    {
        type            calculated;
        value           uniform 0;
    }
    Outlet
    {
        type            calculated;
        value           uniform 0;
    }
    TopBottom
    {
        type            calculated;
        value           uniform 0;
    }
    Wing-Bottom
    {
        type            calculated;
        value           uniform 0;
    }
    Wing-Top
    {
        type            calculated;
        value           uniform 0;
    }
    Base
    {
        type            empty;
        value           nonuniform List<scalar> 0;
    }
    Top
    {
        type            empty;
        value           nonuniform List<scalar> 0;
    }
}


